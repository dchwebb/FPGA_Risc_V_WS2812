                                <ipxact:name>apb_penable_i</ipxact:name>
                            </ipxact:physicalPort>
                        </ipxact:portMap>
                        <ipxact:portMap>
                            <ipxact:logicalPort>
                                <ipxact:name>PWRITE</ipxact:name>
                            </ipxact:logicalPort>
                            <ipxact:physicalPort>
                                <ipxact:name>apb_pwrite_i</ipxact:name>
                            </ipxact:physicalPort>
                        </ipxact:portMap>
                        <ipxact:portMap>
                            <ipxact:logicalPort>
                                <ipxact:name>PRDATA</ipxact:name>
                            </ipxact:logicalPort>
                            <ipxact:physicalPort>
                                <ipxact:name>apb_prdata_o</ipxact:name>
                            </ipxact:physicalPort>
                        </ipxact:portMap>
                        <ipxact:portMap>
                            <ipxact:logicalPort>
                                <ipxact:name>PWDATA</ipxact:name>
                            </ipxact:logicalPort>
                            <ipxact:physicalPort>
                                <ipxact:name>apb_pwdata_i</ipxact:name>
                            </ipxact:physicalPort>
                        </ipxact:portMap>
                        <ipxact:portMap>
                            <ipxact:logicalPort>
                                <ipxact:name>PREADY</ipxact:name>
                            </ipxact:logicalPort>
                            <ipxact:physicalPort>
                                <ipxact:name>apb_pready_o</ipxact:name>
                            </ipxact:physicalPort>
                        </ipxact:portMap>
                        <ipxact:portMap>
                            <ipxact:logicalPort>
                                <ipxact:name>PSLVERR</ipxact:name>
                            </ipxact:logicalPort>
                            <ipxact:physicalPort>
                                <ipxact:name>apb_pslverr_o</ipxact:name>
                            </ipxact:physicalPort>
                        </ipxact:portMap>
                    </ipxact:portMaps>
                </ipxact:abstractionType>
            </ipxact:abstractionTypes>
            <ipxact:slave>
                <ipxact:memoryMapRef memoryMapRef="uart_mem_map"/>
            </ipxact:slave>
        </ipxact:busInterface>
        <ipxact:busInterface>
            <ipxact:name>INT_M0</ipxact:name>
            <ipxact:displayName>INT_M0</ipxact:displayName>
            <ipxact:description>Interrupt Request</ipxact:description>
            <ipxact:busType library="busdef.interrupt" name="interrupt" vendor="spiritconsortium.org" version="1.0"/>
            <ipxact:abstractionTypes>
                <ipxact:abstractionType>
                    <ipxact:abstractionRef library="busdef.interrupt" name="interrupt_rtl" vendor="spiritconsortium.org" version="1.0"/>
                    <ipxact:portMaps>
                        <ipxact:portMap>
                            <ipxact:logicalPort>
                                <ipxact:name>IRQ</ipxact:name>
                            </ipxact:logicalPort>
                            <ipxact:physicalPort>
                                <ipxact:name>int_o</ipxact:name>
                            </ipxact:physicalPort>
                        </ipxact:portMap>
                    </ipxact:portMaps>
                </ipxact:abstractionType>
            </ipxact:abstractionTypes>
            <ipxact:master/>
        </ipxact:busInterface>
    </ipxact:busInterfaces>
    <ipxact:memoryMaps>
        <ipxact:memoryMap>
            <ipxact:name>uart_mem_map</ipxact:name>
            <ipxact:description>UART memory map</ipxact:description>
            <ipxact:addressBlock>
                <ipxact:name>UART_Registers</ipxact:name>
                <ipxact:baseAddress>0</ipxact:baseAddress>
                <ipxact:range>64</ipxact:range>
                <ipxact:width>32</ipxact:width>
                <ipxact:register>
                    <ipxact:name>THR</ipxact:name>
                    <ipxact:displayName>Transmitter_Holding_Register</ipxact:displayName>
                    <ipxact:description>Transmitter Holding Register is the interface to the Transmitter Buffer/FIFO (XMIT FIFO).</ipxact:description>
                    <ipxact:addressOffset>0</ipxact:addressOffset>
                    <ipxact:size>32</ipxact:size>
                    <ipxact:volatile>true</ipxact:volatile>
                    <ipxact:access>write-only</ipxact:access>
                    <ipxact:field>
                        <ipxact:name>XMIT_FIFO</ipxact:name>
                        <ipxact:displayName>XMIT_FIFO</ipxact:displayName>
                        <ipxact:description>Write access to this field pushes data to XMIT FIFO</ipxact:description>
                        <ipxact:bitOffset>0</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>8</ipxact:bitWidth>
                        <ipxact:volatile>true</ipxact:volatile>
                        <ipxact:access>write-only</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">false</ipxact:testable>
                    </ipxact:field>
                </ipxact:register>
                <ipxact:register>
                    <ipxact:name>RBR</ipxact:name>
                    <ipxact:displayName>Receive_Buffer_Register</ipxact:displayName>
                    <ipxact:description>Receive Buffer Register is the interface to the Receiver Buffer/FIFO (RCVR FIFO).</ipxact:description>
                    <ipxact:addressOffset>0</ipxact:addressOffset>
                    <ipxact:size>32</ipxact:size>
                    <ipxact:volatile>true</ipxact:volatile>
                    <ipxact:access>read-only</ipxact:access>
                    <ipxact:field>
                        <ipxact:name>RCVR_FIFO</ipxact:name>
                        <ipxact:displayName>RCVR_FIFO</ipxact:displayName>
                        <ipxact:description>Read access to this field pops data from RCVR FIFO</ipxact:description>
                        <ipxact:bitOffset>0</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                                <ipxact:mask>0</ipxact:mask>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>8</ipxact:bitWidth>
                        <ipxact:volatile>true</ipxact:volatile>
                        <ipxact:access>read-only</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">false</ipxact:testable>
                    </ipxact:field>
                </ipxact:register>
                <ipxact:register>
                    <ipxact:name>IER</ipxact:name>
                    <ipxact:displayName>Interrupt_Enable_Register</ipxact:displayName>
                    <ipxact:description>Interrupt Enable Register enables the four types of UART interrupts.</ipxact:description>
                    <ipxact:addressOffset>4</ipxact:addressOffset>
                    <ipxact:size>32</ipxact:size>
                    <ipxact:volatile>false</ipxact:volatile>
                    <ipxact:access>read-write</ipxact:access>
                    <ipxact:field>
                        <ipxact:name>rda_int_en</ipxact:name>
                        <ipxact:displayName>RDA_Interrupt_Enable</ipxact:displayName>
                        <ipxact:description>Received Data Available Interrupt Enable. 0-&gt; Interrupt is disabled. 1-&gt; Interrupt is enabled.</ipxact:description>
                        <ipxact:bitOffset>0</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:access>read-write</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>thre_int_en</ipxact:name>
                        <ipxact:displayName>THRE_Interrupt_Enable</ipxact:displayName>
                        <ipxact:description>Transmitter Holding Register Empty Interrupt Enable. 0-&gt; Interrupt is disabled. 1-&gt; Interrupt is enabled.</ipxact:description>
                        <ipxact:bitOffset>1</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:access>read-write</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>rls_int_en</ipxact:name>
                        <ipxact:displayName>RLS_Interrupt_Enable</ipxact:displayName>
                        <ipxact:description>Receiver Line Status Interrupt Enable. 0-&gt; Interrupt is disabled. 1-&gt; Interrupt is enabled.</ipxact:description>
                        <ipxact:bitOffset>2</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:access>read-write</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                </ipxact:register>
                <ipxact:register>
                    <ipxact:name>IIR</ipxact:name>
                    <ipxact:displayName>Interrupt_Identification_Register</ipxact:displayName>
                    <ipxact:description>Interrupt Identification Register determines the asserted interrupt based on priority.</ipxact:description>
                    <ipxact:addressOffset>8</ipxact:addressOffset>
                    <ipxact:size>32</ipxact:size>
                    <ipxact:volatile>true</ipxact:volatile>
                    <ipxact:access>read-only</ipxact:access>
                    <ipxact:field>
                        <ipxact:name>int_pending</ipxact:name>
                        <ipxact:displayName>Interrupt_Pending</ipxact:displayName>
                        <ipxact:description>0 -&gt; Interrupt is pending according to int_prio and timeout_int. 1 -&gt; No pending interrupt.</ipxact:description>
                        <ipxact:bitOffset>0</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>1</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>true</ipxact:volatile>
                        <ipxact:access>read-only</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">false</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>int_prio</ipxact:name>
                        <ipxact:displayName>Interrupt_Priority</ipxact:displayName>
                        <ipxact:description>2'b11 -&gt; Receiver Line Status Interrupt. 2'b10 -&gt; Received Data Available Interrupt. 2'b01 -&gt; Transmitter Holding Register Empty Interrupt. 2'b00 -&gt; MODEM Status Interrupt.</ipxact:description>
                        <ipxact:bitOffset>1</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>2</ipxact:bitWidth>
                        <ipxact:volatile>true</ipxact:volatile>
                        <ipxact:access>read-only</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">false</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>fifos_en</ipxact:name>
                        <ipxact:displayName>FIFOs_Enabled</ipxact:displayName>
                        <ipxact:description>FIFO mode - Fixed to logic 2'b11. Non-FIFO mode - fixed to 2'b00.</ipxact:description>
                        <ipxact:bitOffset>6</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>2</ipxact:bitWidth>
                        <ipxact:volatile>true</ipxact:volatile>
                        <ipxact:access>read-only</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">false</ipxact:testable>
                    </ipxact:field>
                </ipxact:register>
                <ipxact:register>
                    <ipxact:name>LCR</ipxact:name>
                    <ipxact:displayName>Line_Control_Register</ipxact:displayName>
                    <ipxact:description>Line Control Register configures character length, number stop bits and parity bit.</ipxact:description>
                    <ipxact:addressOffset>12</ipxact:addressOffset>
                    <ipxact:size>32</ipxact:size>
                    <ipxact:volatile>false</ipxact:volatile>
                    <ipxact:access>read-write</ipxact:access>
                    <ipxact:field>
                        <ipxact:name>char_len_sel</ipxact:name>
                        <ipxact:displayName>Character_Length_Select</ipxact:displayName>
                        <ipxact:description> 2'b00 -&gt; 5 bits. 2'b01 -&gt; 6 bits. 2'b10 -&gt; 7 bits. 2'b11 -&gt; 8 bits.</ipxact:description>
                        <ipxact:bitOffset>0</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>3</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>2</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:access>read-write</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>stop_bit_ctrl</ipxact:name>
                        <ipxact:displayName>Stop Bit Control</ipxact:displayName>
                        <ipxact:description>0 -&gt; One stop bit is generated in the transmitted data. 1 -&gt; Two stop bits are generated.</ipxact:description>
                        <ipxact:bitOffset>2</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:access>read-write</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>parity_en</ipxact:name>
                        <ipxact:displayName>Parity_Enable</ipxact:displayName>
                        <ipxact:description>Parity Enable. 0-&gt; Parity generation and checking is disabled. 1-&gt; Parity generation and checking is enabled.</ipxact:description>
                        <ipxact:bitOffset>3</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>False</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:access>read-write</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>even_parity_sel</ipxact:name>
                        <ipxact:displayName>Even_Parity_select</ipxact:displayName>
                        <ipxact:description>Even Parity Select. 0 -&gt; Odd parity. 1 -&gt; Even Parity.</ipxact:description>
                        <ipxact:bitOffset>4</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>1</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:access>read-write</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>stick_parity_en</ipxact:name>
                        <ipxact:displayName>Stick_Parity_Enable</ipxact:displayName>
                        <ipxact:description>Stick Parity Enable. 0 -&gt; Stick parity is disabled. 1 -&gt; Stick parity is enabled.</ipxact:description>
                        <ipxact:bitOffset>5</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>False</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:access>read-write</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>break_ctrl_en</ipxact:name>
                        <ipxact:displayName>Break_Control_Enable</ipxact:displayName>
                        <ipxact:description>Break Control Enable. 0 -&gt; Break control is disabled. 1 -&gt; Break control is enabled.</ipxact:description>
                        <ipxact:bitOffset>6</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>false</ipxact:volatile>
                        <ipxact:access>read-write</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                </ipxact:register>
                <ipxact:register>
                    <ipxact:name>LSR</ipxact:name>
                    <ipxact:displayName>Line_Status_Register</ipxact:displayName>
                    <ipxact:description>Line Status Register</ipxact:description>
                    <ipxact:addressOffset>20</ipxact:addressOffset>
                    <ipxact:size>32</ipxact:size>
                    <ipxact:volatile>true</ipxact:volatile>
                    <ipxact:access>read-only</ipxact:access>
                    <ipxact:field>
                        <ipxact:name>data_rdy</ipxact:name>
                        <ipxact:displayName>Data_Ready</ipxact:displayName>
                        <ipxact:description>Data Ready Indicator asserts when RCVR FIFO has data.</ipxact:description>
                        <ipxact:bitOffset>0</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>true</ipxact:volatile>
                        <ipxact:access>read-only</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>overrun_err</ipxact:name>
                        <ipxact:displayName>Overrun_Error</ipxact:displayName>
                        <ipxact:description>Overrun Error Indicator asserts when RCVR FIFO is full and the next character has been fully received.</ipxact:description>
                        <ipxact:bitOffset>1</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>true</ipxact:volatile>
                        <ipxact:access>read-only</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>parity_err</ipxact:name>
                        <ipxact:displayName>Parity_Error</ipxact:displayName>
                        <ipxact:description>Parity Error Indicator asserts when parity error occurs.</ipxact:description>
                        <ipxact:bitOffset>2</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>true</ipxact:volatile>
                        <ipxact:access>read-only</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>framing_err</ipxact:name>
                        <ipxact:displayName>Framing_Error</ipxact:displayName>
                        <ipxact:description>Framing Error Indicator asserts when received data does not have a valid stop bit.</ipxact:description>
                        <ipxact:bitOffset>3</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>true</ipxact:volatile>
                        <ipxact:access>read-only</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
                        <ipxact:name>break_cond</ipxact:name>
                        <ipxact:displayName>Break_Condition</ipxact:displayName>
                        <ipxact:description>Break condition indicator asserts when rxd_i is held low for longer than full RX time.</ipxact:description>
                        <ipxact:bitOffset>4</ipxact:bitOffset>
                        <ipxact:resets>
                            <ipxact:reset>
                                <ipxact:value>0</ipxact:value>
                            </ipxact:reset>
                        </ipxact:resets>
                        <ipxact:bitWidth>1</ipxact:bitWidth>
                        <ipxact:volatile>true</ipxact:volatile>
                        <ipxact:access>read-only</ipxact:access>
                        <ipxact:testable testConstraint="unconstrained">true</ipxact:testable>
                    </ipxact:field>
                    <ipxact:field>
